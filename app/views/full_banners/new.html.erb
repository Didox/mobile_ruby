<h1>New full_banner</h1>

<% form_for @full_banner, :html => {:multipart => true}  do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :file %><br />
    <%= f.file_field :file %>
  </p>
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :date_begin %><br />
    <%= f.datetime_select :date_begin %>
  </p>
  <p>
    <%= f.label :date_end %><br />
    <%= f.datetime_select :date_end %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', full_banners_path %>
